      "targets": [
        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH etf_returns AS (\n  SELECT \n    s.symbol,\n    s.company_name,\n    ARRAY_AGG(((td.price - LAG(td.price) OVER (PARTITION BY td.asset_id ORDER BY td.timestamp)) / LAG(td.price) OVER (PARTITION BY td.asset_id ORDER BY td.timestamp)) ORDER BY td.timestamp) as returns\n  FROM market_data.tick_data td\n  JOIN market_data.stocks s ON s.id = td.asset_id\n  WHERE \n    td.asset_type = 'stock' AND\n    td.timestamp >= NOW() - INTERVAL '90 days' AND\n    s.symbol ~ '^[A-Z]{3,4}$' AND\n    s.company_name ILIKE '%ETF%'\n  GROUP BY s.symbol, s.company_name\n  HAVING COUNT(*) > 50\n),\nvar_calculations AS (\n  SELECT \n    symbol,\n    company_name,\n    PERCENTILE_CONT(0.05) WITHIN GROUP (ORDER BY unnest(returns)) as var_5,\n    PERCENTILE_CONT(0.95) WITHIN GROUP (ORDER BY unnest(returns)) as var_95,\n    PERCENTILE_CONT(0.01) WITHIN GROUP (ORDER BY unnest(returns)) as var_99\n  FROM etf_returns\n)\nSELECT \n  symbol,\n  company_name,\n  ROUND((var_5 * 100)::numeric, 3) as \"VaR 5%\",\n  ROUND((var_95 * 100)::numeric, 3) as \"VaR 95%\",\n  ROUND((var_99 * 100)::numeric, 3) as \"VaR 99%\",\n  ROUND(((var_5 * 1.2) * 100)::numeric, 3) as \"CVaR 5%\"\nFROM var_calculations\nORDER BY var_5\nLIMIT 74",
          "refId": "A"
        }
