        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH daily_returns AS (\n  SELECT \n    td.asset_type,\n    DATE(td.timestamp) as trade_date,\n    FIRST_VALUE(td.price) OVER (PARTITION BY td.asset_id, td.asset_type, DATE(td.timestamp) ORDER BY td.timestamp) as open_price,\n    LAST_VALUE(td.price) OVER (PARTITION BY td.asset_id, td.asset_type, DATE(td.timestamp) ORDER BY td.timestamp ROWS BETWEEN UNBOUNDED PRECEDING AND UNBOUNDED FOLLOWING) as close_price\n  FROM market_data.tick_data td\n  WHERE td.timestamp >= NOW() - INTERVAL '30 days'\n),\nreturns_calc AS (\n  SELECT \n    asset_type,\n    trade_date,\n    (close_price - open_price) / open_price * 100 as daily_return\n  FROM daily_returns\n  WHERE open_price > 0\n)\nSELECT \n  asset_type,\n  ROUND(AVG(daily_return)::numeric, 2) as avg_return\nFROM returns_calc\nGROUP BY asset_type",
          "refId": "A"
        }
      ],
      "title": "Average Return Rates by Category (30 Days)",
      "type": "stat"
